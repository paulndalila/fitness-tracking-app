{
  "expo": {
    "name": "fitness-tracking-app",
    "slug": "fitness-tracking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fitness.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fitness.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.paul_ndalila.fitnesstrackingapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/fitness.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fitness.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7b55fbb0-6460-4a9f-987a-5436a37afc9c"
      }
    }
  }
}
